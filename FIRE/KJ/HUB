    -- Obtém o jogador local
local player = game.Players.LocalPlayer

-- Obtém o personagem do jogador
local character = player.Character or player.CharacterAdded:Wait()

-- Obtém o humanoide do personagem
local humanoid = character:WaitForChild("Humanoid")

-- ID da animação que queremos monitorar
local animationId = "16945550029"

-- Cria um objeto de animação
local animation = Instance.new("Animation")
animation.AnimationId = "rbxassetid://" .. animationId

-- Cria uma animação do humanoide
local animator = Instance.new("Animator")
animator.Name = "Animator"
animator.Parent = humanoid

-- Cria uma animação e adiciona ao Animator
local animTrack = humanoid:LoadAnimation(animation)

-- Função para detectar quando a animação é executada
local function onAnimationPlayed(track)
    if track.Animation.AnimationId == "rbxassetid://" .. animationId then
        -- Adicione aqui a animação que você quer iniciar
        print("A animação especificada está sendo executada!")

        -- Exemplo: Reproduz uma animação personalizada
        local newAnimation = Instance.new("Animation")
        newAnimation.AnimationId = "rbxassetid://16945550029"
        local newAnimTrack = humanoid:LoadAnimation(newAnimation)
        newAnimTrack:Play()
    end
end

-- Conecte a função ao evento de animação
animator.AnimationPlayed:Connect(onAnimationPlayed)

-- Configura o rastreamento de animação
humanoid.AnimationPlayed:Connect(onAnimationPlayed)
-- LocalScript para Roblox

